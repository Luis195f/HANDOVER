{
  "cli": { "version": ">= 15.0.0", "appVersionSource": "remote" },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "env": { "EXPO_NO_INPUT": "1" }
    },
    "preview": {
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "env": { "EXPO_NO_INPUT": "1" }
    },
    "production": {
      "autoIncrement": true,
      "env": { "EXPO_NO_INPUT": "1" }
    }
  },
  "submit": { "production": {} }
}



