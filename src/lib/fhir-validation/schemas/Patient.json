{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Patient",
  "type": "object",
  "required": ["resourceType"],
  "properties": {
    "resourceType": { "const": "Patient" },
    "id": { "type": "string" },
    "identifier": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "system": { "type": "string" },
          "value": { "type": "string" }
        },
        "additionalProperties": true
      }
    },
    "name": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "text": { "type": "string" },
          "family": { "type": "string" },
          "given": {
            "type": "array",
            "items": { "type": "string" }
          }
        },
        "additionalProperties": true
      }
    },
    "gender": { "type": "string" },
    "birthDate": { "type": "string" }
  },
  "additionalProperties": true
}
